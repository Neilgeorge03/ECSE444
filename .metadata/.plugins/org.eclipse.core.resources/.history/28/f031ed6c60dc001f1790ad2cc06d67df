#ifndef INC_KALMANFILTERASSEMBLY_H_
#define INC_KALMANFILTERASSEMBLY_H_

typedef struct {
    float q;
    float r;
    float x;
    float p;
    float k;
} kalman_state;

void kalmanFilter(struct kalman_state* state, float measurement);

#endif /* INC_KALMANFILTERASSEMBLY_H_ */

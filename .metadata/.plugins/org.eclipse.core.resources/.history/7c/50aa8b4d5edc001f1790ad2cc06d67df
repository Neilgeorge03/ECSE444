#ifndef INC_KALMANFILTER_C_CMSIS_H_
#define INC_KALMANFILTER_C_CMSIS_H_


typedef struct {
    float32_t q;
    float32_t r;
    float32_t x;
    float32_t p;
    float32_t k;
} kalman_state;

void KalmanFilter_C_CMSIS (kalman_state* kState, float32_t measurement);

#endif /* INC_KALMANFILTER_C_CMSIS_H_ */
